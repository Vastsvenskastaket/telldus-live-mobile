<?xml version="1.0" encoding="utf-8" standalone="no"?>
<qnx xmlns="http://www.qnx.com/schemas/application/1.0">
	<id>com.telldus.tellduscenterlight</id>
	<name>TelldusCenter light</name>
	<author>Telldus Technologies AB</author>
	<versionNumber>1.0.0</versionNumber>
	<buildIdFile>_buildId</buildIdFile>
	<description></description>
	<initialWindow>
		<autoOrients>true</autoOrients>
	</initialWindow>
	<icon><image>icon.png</image></icon>
	<asset path="TelldusCenter-light" entry="true" type="Qnx/Elf">TelldusCenter-light</asset>
	<permission system="true">run_native</permission>
	<invoke-target id="com.telldus.TARGET">
		<filter>
			<action>bb.action.VIEW</action>
			<action>bb.action.OPEN</action>
			<mime-type>*</mime-type>
			<property var="uris" value="x-com-telldus-tellduscenter://"/>
		</filter>
	</invoke-target>
</qnx>
